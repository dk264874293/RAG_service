# ==================== OpenAI 配置 ====================
OPENAI_MODEL_NAME=gpt-3.5-turbo
OPENAI_API_BASE=https://api.openai.com/v1
OPENAI_API_BASE=https://api.openai.com/v1

# ==================== 数据库配置 ====================
# MySQL数据库连接URL
# 格式: mysql+aiomysql://username:password@host:port/database_name
# 示例: mysql+aiomysql://root:yourpassword@localhost:3306/rag_service
DATABASE_URL=mysql+aiomysql://root:password@localhost:3306/rag_service

# ==================== Redis 配置 ====================
REDIS_SERVER_HOST=localhost
REDIS_SERVER_PORT=6379
REDIS_SERVER_DB=3

# ==================== OCR 配置 ====================
OCR_ENGINE=PaddleOCR-VL
OCR_API_ENDPOINT=https://your-ocr-api-endpoint.com/layout-parsing
OCR_API_KEY=your-ocr-api-key-here

# ==================== DashScope 配置 ====================
DASHSCOPE_API_KEY=your-dashscope-api-key-here

# ==================== 安全配置 ====================
# JWT密钥，生产环境必须使用强随机密钥
SECRET_KEY=your-secret-key-change-in-production
API_KEY=your-api-key-here

# ==================== 其他配置 ====================
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# ==================== 调试配置 ====================
# 是否启用调试模式
DEBUG_MODE=True
